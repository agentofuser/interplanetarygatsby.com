{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "gatsby": "^2.3.3",
    "gatsby-image": "^2.0.35",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@agentofuser/ipfs-deploy": "^7.1.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/prompt": "^7.5.0",
    "@types/node": "^11.13.7",
    "@types/react-helmet": "^5.0.8",
    "@types/typography": "^0.16.2",
    "commitizen": "^3.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-feed": "^2.1.0",
    "gatsby-plugin-ipfs": "^2.0.2",
    "gatsby-plugin-manifest": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.28",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "ndb": "^1.0.44",
    "nodemon": "^1.18.11",
    "prettier": "^1.16.4",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "type-check": "tsc --noEmit --project tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "build": "rm -rf .cache/ public/ && gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write ./**/*.{js,ts,tsx,json,md,mdx,html,css}",
    "start": "yarn develop",
    "debug-start": "ndb npm run start",
    "serve": "gatsby serve",
    "deploy": "ipfs-deploy -O -p infura -p pinata -d cloudflare",
    "commit": "git-cz",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "commit:retry": "git-cz --retry",
    "commitmsg": "commitlint -e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "type-check",
      "pre-commit": "lint",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,mdx,html,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  }
}
